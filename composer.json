{
    "name": "imemento/http-responses",
    "description": "HTTP Responses",
    "homepage": "https://gitlab.com/imemento/composer/packages/http-responses",
    "license": "MIT",
    "authors": [
        {
            "name": "George Stan",
            "email": "contact@georgestan.com",
            "homepage": "http://imemento.com"
        }
    ],
    "require": {
        "php": "^7.1",
        "symfony/http-foundation": "^4.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "^7.0"
    },
    "autoload": {
        "psr-4": {
            "iMemento\\Http\\Responses\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "iMemento\\Http\\Responses\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "repositories": [
        {"type": "composer", "url": "https://repo.packagist.com/imemento/"},
        {"packagist.org": false}
    ],
    "minimum-stability": "dev",
    "prefer-stable": true
}
